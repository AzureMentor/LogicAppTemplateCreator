    <command:details>
      <command:name>Get-LogicAppTemplate</command:name>
      <command:verb>Get</command:verb>
      <command:noun>LogicAppTemplate</command:noun>
      <maml:description>
        <maml:para>Generate LogicApp ARM template</maml:para>
      </maml:description>
    </command:details>
